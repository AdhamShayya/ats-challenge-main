{
    "version": 2,
    "builds": [
      {
        "src": "ats-challenge-main/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "ats-backend/src/app.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "ats-backend/src/app.ts"
      },
      {
        "src": "/(.*)",
        "dest": "ats-challenge-main"
      }
    ]
  }